<div id="movements">
  <table class="table table-striped table-bordered table-condensed">
    <thead>
      <tr>
        <th>Taken By</th>
        <th>Taken At</th>
        <th>Returned At</th>
    </thead>
    <tbody>
      <tr>
        <% movements.each do |movement| %>
            <td><%= movement.taken_by %></td>
            <td><%= movement.taken_at %></td>
            <td><%= movement.reurned_at %></td>
        <% end %>
      </tr>
    </tbody>
  </table>
</div>